#include <stdio.h>
#include <vector>

#include <conio.h>

using namespace std;


int main() {
    
    
    int N;
    scanf("%d", &N);
    
    int res = N;
    printf("%d ", res);
    
    while(res != 1) {
        
        if(res %2 ==0) {
            res/=2;
               
        }else {
            res = (res*3+1);
        }   
        printf("%d " , res );
    }
    
    
    
    getch();
    
    
 return 0;   
}
