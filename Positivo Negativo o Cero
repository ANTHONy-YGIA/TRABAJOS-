#include <iostream>
#include <stdio.h>
#include <vector>
#include <algorithm>

#include <conio.h>

using namespace std;


int main() {
  
  int N;
  scanf("%d", &N);
  
  if(N > 0) {
        printf("POSITIVO");   
    }else if(N < 0) {
        printf("NEGATIVO");   
    }else {
        printf("CERO");
    }   
    printf("\n");
  
  getch();
    
  return 0;   
}
