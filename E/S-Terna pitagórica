#include <iostream>
#include <stdio.h>
#include <vector>

#include <conio.h>

using namespace std;

int main() {
    
    
    int a,b,c;
    scanf("%d %d %d", &a, &b, &c);
    
    if(a * a + b * b == c * c) {
        printf("SI\n");   
    }else {
        printf("NO\n");
    }
    
    getch();
    
    
 return 0;   
}

