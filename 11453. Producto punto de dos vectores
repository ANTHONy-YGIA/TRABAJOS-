#include <iostream>

int main() {

  int n;
  std::cin>>n;
  int array1[n];
  int array2[n];
  for (int i = 0; i < n; ++i){
    std::cin>>array1[i];
  }
  for (int i = 0; i < n; ++i){
    std::cin>>array2[i];
  }
  int resultado = 0;
  for (int i = 0; i < n; ++i){
    resultado += array1[i] * array2[i];
  }
  std::cout<<resultado;

  return 0;
}
