#include <iostream>
#include <stdio.h>
#include <math.h>

#define ll long long int

using namespace std;

int main() {
    
   
   for(int i =1; i<=1000; i++) {
      
      if(i%3 ==0 && i%5==0) {
          printf("FizzBuzz\n");
      } else if(i %3==0) {
          printf("Fizz\n");   
        } else if (i%5==0) {
          printf("Buzz\n");   
        } else {
           printf("%d\n",i);   
        } 
    }
   
   
   system("pause");
    
 return 0;   
}
