#include <iostream>
#include <stdio.h>
#include <vector>
#include <algorithm>

#define ll long long int

using namespace std;


int main() {
    
     int a,b,c;
     scanf("%d %d %d", &a, &b, &c);
     
     int terna[]= {a,b,c};
     std::sort(terna, terna + 3);
     
     /*
     for(int i =0; i<3; i++) {
        printf("%d ", terna[i]);       
     }*/
     
     if(terna[0] * terna[0] + terna[1] * terna[1] == terna[2] * terna[2]) {
          printf("Si\n");   
      }else {
          printf("No\n");   
      }
     
  
    system("pause");

 return 0;   
}
