#include <iostream>
#include <stdio.h>
#include <math.h>

#define ll long long int

using namespace std;

int main() {
    
   int h,m,s;
   scanf("%d %d %d", &h, &m, &s);
   
   s++;
   if(s == 60) {
      s =0;
      m++;   
    }
    if(m == 60) {
      m=0;
      h++;   
    }
    if(h == 24) {
      h=0;   
    }
   
   printf("%d %d %d\n", h,m,s);
   
   
   system("pause");
    
 return 0;   
}
