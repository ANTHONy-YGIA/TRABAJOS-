#include <iostream>
#include <stdio.h>

#include <conio.h>

using namespace std;


int main() {
    
    

    int N;
    scanf("%d", &N);
    
    int mayor = -2000000;
    
    for(int i = 0; i < N; i++) {
        int elem;
        scanf("%d",&elem);
        
        mayor = std::max(mayor, elem);
        
    }

    printf("%d", mayor);


    printf("\n");   
    getch();
    return 0;   
}
