#include <iostream>
#include <stdio.h>
#include <vector>
#include <algorithm>

#define ll long long int

using namespace std;


int main() {
    
   int n;
   scanf("%d", &n);
    
   int ans =0;
   
   for(int i =0; i<n; i++) {
 
     float a;
     scanf("%f", &a);
     
     if(a > 1.7) {
          ans ++;  
     }
   }
   
    printf("%d\n", ans);
    
    system("pause");

 return 0;   
}

